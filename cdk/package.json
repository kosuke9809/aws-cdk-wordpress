{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "diff": "cdk diff --profile kubell",
    "deploy": "cdk deploy --all --profile kubell",
    "destroy": "cdk destroy --all --profile kubell",
    "synth": "cdk synth --profile kubell"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.7.9",
    "aws-cdk": "2.1007.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.29.1"
  },
  "dependencies": {
    "aws-cdk-lib": "2.186.0",
    "constructs": "^10.0.0"
  }
}